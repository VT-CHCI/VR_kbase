<h1>Listing finding_controls</h1>

<table>
  <tr>
    <th>Finding</th>
    <th>Control symmetry</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @finding_controls.each do |finding_control| %>
  <tr>
    <td><%= finding_control.finding %></td>
    <td><%= finding_control.control_symmetry %></td>
    <td><%= link_to 'Show', finding_control %></td>
