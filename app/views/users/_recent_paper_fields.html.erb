<div id="paper-list" class="left">
  <div class="row-fluid">
    <div class="span12">
      <div class="row-fluid">
        <div class="span6"><legend>Most Recent Submissions</legend></div>
      </div>
      <% @papers.take(5).each do |paper| %>
        <div class="span6">
          <%= render 'paper_fields', :paper => paper %>
        </div>
      <% end %>
    </div>
  </div>
</div>