<h1>Listing finding_relationships</h1>

<table>
  <tr>
    <th>Relationship</th>
    <th>Finding</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @finding_relationships.each do |finding_relationship| %>
  <tr>
    <td><%= finding_relationship.relationship %></td>
    <td><%= finding_relationship.finding %></td>
    <td><%= link_to 'Show', finding_relationship %></td>
