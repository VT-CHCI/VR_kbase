<h1>Listing papers</h1>

<table>
  <tr>
    <th>Experiments</th>
    <th>Title</th>
    <th>Year</th>
    <th>Volume</th>
    <th>Num pages</th>
    <th>Start page</th>
    <th>Doi</th>
    <th>Paper url</th>
    <th>Num views</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @papers.each do |paper| %>
  <tr>
    <td><%= paper.experiments %></td>
    <td><%= paper.title %></td>
    <td><%= paper.year %></td>
    <td><%= paper.volume %></td>
    <td><%= paper.end_page %></td>
    <td><%= paper.start_page %></td>
    <td><%= paper.doi %></td>
    <td><%= paper.paper_url %></td>
    <td><%= paper.num_views %></td>
    <td><%= link_to 'Show', paper %></td>
    <td><%= link_to 'Edit', edit_paper_path(paper) %></td>
    <td><%= link_to 'Destroy', paper, :method => :delete, :data => { :confirm => 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Paper', new_paper_path %>
