<h1>Listing tasks</h1>

<table>
  <tr>
    <th>Experiment</th>
    <th>Title</th>
    <th>Task desc</th>
    <th>Interface desc</th>
    <th>Env desc</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @tasks.each do |task| %>
  <tr>
    <td><%= task.experiment %></td>
    <td><%= task.title %></td>
    <td><%= task.task_desc %></td>
    <td><%= task.interface_desc %></td>
    <td><%= task.env_desc %></td>
    <td><%= link_to 'Show', task %></td>
    <td><%= link_to 'Edit', edit_task_path(task) %></td>
    <td><%= link_to 'Destroy', task, :method => :delete, :data => { :confirm => 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Task', new_task_path %>
